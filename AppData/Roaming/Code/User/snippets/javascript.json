	"For-Of Loop2": {
		"prefix": "for (var ${1} of ${2:array})",
		"body": [
		  "for (var ${1:iterator} of ${2:array}) {",
		  "\t$TM_SELECTED_TEXT$0",
		  "}"
		],
	  },


	"string.if-empty": {
